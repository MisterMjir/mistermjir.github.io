---
---
<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0"
     xmlns:content="http://purl.org/rss/1.0/modules/content/"
     xmlns:atom="http://www.w3.org/2005/Atom"
     xmlns:media="http://search.yahoo.com/mrss/">

<channel>
  <title>Mister Mjir's Blog</title>
  <link>https://mistermjir.github.io/latest.html</link>
  <description>All my blogs</description>
  <atom:link href="https://mistermjir.github.io/feeds/all.xml" rel="self" type="application/rss+xml" />
  {% for post in site.posts %}
  <item>
    <title>{{ post.title }}</title>
    <link>https://mistermjir.github.io/{{ post.url }}</link>
    <guid isPermaLink="true">https://mistermjir.github.io/{{ post.url }}</guid>
    <description>An article about {{ post.title }}</description>
    <content:encoded><![CDATA[{{ post.content }}]]></content:encoded>
    <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
  </item>
  {% endfor %}
</channel>

</rss> 
